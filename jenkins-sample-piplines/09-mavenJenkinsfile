pipeline {
    agent any
    stages {
        stage('Maven Project') {
            steps {
                git credentialsId: 'agnes', url: "git@github.com:Mettilda-89/DEVOPS--TRAINING.git", branch: 'maven'
                sh "git checkout maven"
                dir('MavenHelloWorldProject') {
                    sh "mvn clean install"
                }
                post {
                    success {
                        archiveArtifacts "MavenHelloWorldProject/target/*.jar"
                    }
                }
            }
        }
    }
}
